				type = "passive";
			};
		};
	};

	rf-back-therm {
		polling-delay-passive = <0>;
		polling-delay = <0>;
		thermal-governor = "user_space";
		thermal-sensors = <&pm8150_adc_tm ADC_AMUX_THM2_PU2>;
		wake-capable-sensor;
		trips {
			active-config0 {
				temperature = <125000>;
				hysteresis = <1000>;
				type = "passive";
			};
		};
	};

	sdm-therm {
		polling-delay-passive = <0>;
		polling-delay = <0>;
		thermal-governor = "user_space";
		thermal-sensors = <&pm8150l_adc_tm ADC_AMUX_THM3_PU2>;
		wake-capable-sensor;
		trips {
			active-config0 {
				temperature = <125000>;
				hysteresis = <1000>;
				type = "passive";
			};
		};
	};

	skin-sdm {
		polling-delay-passive = <2000>;
		polling-delay = <0>;
		thermal-governor = "step_wise";
		wake-capable-sensor;
		trips {
			sdm_light: sdm-light {
				temperature = <63000>;
				hysteresis = <2000>;
				type = "passive";
			};
			sdm_moderate_lv1: sdm-moderate-lv1 {
				temperature = <65000>;
				hysteresis = <2000>;
				type = "passive";
			};
			sdm_moderate_lv2: sdm-moderate-lv2 {
				temperature = <67000>;
				hysteresis = <2000>;
				type = "passive";
			};
		};
		cooling-maps {
			/* sdm light throttling */
			cpu7-sdm-light-throttling {
				trip = <&sdm_light>;
				cooling-device =
				<&CPU7 1 1>;
			};
			/* sdm moderate throttling level 1 */
			cpu4-sdm-moderate-lv1-throttling {
				trip = <&sdm_moderate_lv1>;
				cooling-device =
				<&CPU4 1 2>;
			};
			cpu7-sdm-moderate-lv1-throttling {
				trip = <&sdm_moderate_lv1>;
				cooling-device =
				<&CPU7 2 4>;
			};
			/* sdm moderate throttling level 2 */
			cpu0-sdm-moderate-lv2-throttling {
				trip = <&sdm_moderate_lv2>;
				cooling-device =
				<&CPU0 1 2>;
			};
			cpu4-sdm-moderate-lv2-throttling {
				trip = <&sdm_moderate_lv2>;
				cooling-device =
				<&CPU4 2 3>;
			};
			cpu7-sdm-moderate-lv2-throttling {
				trip = <&sdm_moderate_lv2>;
				cooling-device =
				<&CPU7 4 5>;
			};
		};
	};

	usbc-therm-adc {
		polling-delay-passive = <0>;
		polling-delay = <0>;
		thermal-governor = "step_wise";
		thermal-sensors = <&pm8150b_adc_tm ADC_AMUX_THM3_PU2>;
		wake-capable-sensor;
		trips {
			usb_throttling_max: usb-throttling_max {
				temperature = <48000>;
				hysteresis = <10000>;
				type = "passive";
			};
		};
		cooling-maps {
			usb_throttling_max {
				trip = <&usb_throttling_max>;
				cooling-device =
					<&google_overheat_mitigation
					THERMAL_MAX_LIMIT THERMAL_MAX_LIMIT>;
			};
		};
	};

	usbc-therm-monitor {
		polling-delay-passive = <0>;
		polling-delay = <0>;
		thermal-governor = "user_space";
		thermal-sensors = <&pm8150b_adc_tm ADC_AMUX_THM3_PU2>;
		wake-capable-sensor;
		trips {
			active-config0 {
				temperature = <125000>;
				hysteresis = <1000>;
				type = "passive";
			};
		};
	};

	soc {
		polling-delay-passive = <60000>;
		polling-delay = <0>;
		thermal-governor = "low_limits_cap";
		thermal-sensors = <&bcl_soc>;
		tracks-low;
		wake-capable-sensor;

		trips {
			soc_tips: soc-trips {
				temperature = <10>;
				hysteresis = <0>;
				type = "passive";
			};
		};
		cooling-maps {
			soc_cpu4 {
				trip = <&soc_tips>;
				cooling-device =
					<&CPU4 11 11>;
			};
			soc_cpu7 {
				trip = <&soc_tips>;
				cooling-device =
					<&CPU7 11 11>;
			};
			soc_gpu0 {
				trip = <&soc_tips>;
				cooling-device = <&msm_gpu 4 4>;
			};
			soc_npu {
				trip = <&soc_tips>;
				cooling-device =
					<&msm_npu (THERMAL_MAX_LIMIT-3)
					(THERMAL_MAX_LIMIT-3)>;
			};
		};
	};

	pm8150b-bcl-lvl0 {
		polling-delay-passive = <1000>;
		cooling-maps {
			bcl_cpu4 {
				trip = <&b_bcl_lvl0>;
